      <varlistentry>
        <term><literal>\gset [ <replaceable class="parameter">prefix</replaceable> ]</literal></term>

        <listitem>
<!-- pgdoc-cn_start sig_en=c6b2e26e0eec9673c5bb55ac3aa071ba sig_cn_org=ba0df9daf8e86ec44ac9c9dd2a1cd980 source=15.7 
        <para>
