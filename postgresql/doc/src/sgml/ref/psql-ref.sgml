<!-- pgdoc-cn_start sig_en=f55b73b662cbdf36f5305f63bdae1e80 sig_cn_org=None source=14.1 
      <term>
       <varname>AUTOCOMMIT</varname>
       <indexterm>
        <primary>autocommit</primary>
