      <varlistentry>
        <term><varname>QUIET</varname></term>
        <listitem>
<!-- pgdoc-cn_start sig_en=0e5fe8851a0b2fbdb85b4e99440fd216 sig_cn_org=None source=14.1 
        <para>
        Setting this variable to <literal>on</literal> is equivalent to the command
